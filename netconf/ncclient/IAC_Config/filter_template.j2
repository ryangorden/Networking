<config>
  <interfaces xmlns="urn:ietf:params:xml:ns:yang:ietf-interfaces"  xmlns:if="urn:ietf:params:xml:ns:yang:ietf-interfaces">
    <interface>
    <name>{{ name }}</name>
    <ipv4 xmlns="urn:ietf:params:xml:ns:yang:ietf-ip">
      <address>
        <ip>{{ ip }}</ip>
        <netmask>{{ netmask }}</netmask>
      </address>
    </ipv4>
    </interface>
  </interfaces>
</config>

